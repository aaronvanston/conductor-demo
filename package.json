{
  "name": "demorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "biome format --write .",
    "lint:fix": "biome check --write .",
    "check-types": "turbo run check-types",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "husky": "^9.1.7",
    "turbo": "^2.7.3",
    "typescript": "5.9.3",
    "ultracite": "7.0.10"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
